      Name: "Monitor",
      SourceCode: "//RunAt:Desktop\r\nbool IController.Execute()\r\n{\r\n  k.SetupFuncMonitor();\r\n  \r\n  k.TestSuiteName = \"Shopping_Cart_CBM\";\r\n  k.RunDependingTests(k.NewTest(A_AddProducts,A_BeforeAddProducts), k.NewTest(B_ShoppingCart), k.NewTest(C_IncreaseProducts),  k.NewTest(D_ValidateTotals),  k.NewTest(E_ClearProducts));\r\n  k.Run(\"Monitor>Kill\");\r\n  return true;\r\n}\r\n\r\nprivate void A_BeforeAddProducts()\r\n{\r\n  k.RunDependingActionsWithRetry(2, \"AddProducts>Navigate\", \"AddProducts>Clic\");\r\n}\r\n\r\n[MetricsInfo(ExcellentTime = 20000,  ToleratingTime = 40000)]\r\nprivate void A_AddProducts()\r\n{\r\n  //k.Run(\"AddProducts>Navigate\");\r\n  //k.Run(\"AddProducts>Clic\");\r\n  k.Run(\"AddProducts>WriteProduct1\");\r\n  k.Run(\"AddProducts>SearchProducts\");\r\n  k.Run(\"AddProducts>ClicProducts\");\r\n  k.Run(\"AddProducts>NameProduct1\");\r\n  k.Run(\"AddProducts>AddCart1\");\r\n  k.RunAsyncVoid(\"AddProducts>Close\");\r\n  k.Run(\"AddProducts>Clic\");\r\n  k.Run(\"AddProducts>WriteProduct2\");\r\n  k.Run(\"AddProducts>SearchProducts\");\r\n  k.Run(\"AddProducts>ClicProducts\");\r\n  k.Run(\"AddProducts>NameProduct2\");\r\n  k.Run(\"AddProducts>AddCart2\");\r\n  k.RunAsyncVoid(\"AddProducts>Close\");\r\n}\r\n\r\n[MetricsInfo(ExcellentTime = 2000,  ToleratingTime = 4000)]\r\nprivate void B_ShoppingCart()\r\n{ \r\n  k.Run(\"ShoppingCart\", true);\r\n}\r\n\r\n[MetricsInfo(ExcellentTime = 4000,  ToleratingTime = 8000)]\r\nprivate void C_IncreaseProducts()\r\n{\r\n  k.Run(\"IncreaseProducts\", true);\r\n}\r\n\r\n[MetricsInfo(ExcellentTime = 2000,  ToleratingTime = 4000)]\r\nprivate void D_ValidateTotals()\r\n{ \r\n  k.Sleep(k.USER_THINK_TIME);\r\n  k.Run(\"ValidateTotals>ExtractValueTotals\");\r\n  k.Run(\"ValidateTotals>ExtractValueTotal\");\r\n  k.ValidationPrices(); \r\n}\r\n\r\n[MetricsInfo(ExcellentTime = 2000,  ToleratingTime = 4000)]\r\nprivate void E_ClearProducts()\r\n{\r\n  k.Run(\"ClearProducts\", true);\r\n  k.LogTextToGH(\"Deleted Products\", \"He shopping cart has 0 items now\");\r\n}",
