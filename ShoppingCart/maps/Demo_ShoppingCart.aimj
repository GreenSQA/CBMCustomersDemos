  GlobalSourceCode: "readonly int USER_THINK_TIME = 1000;\r\n\r\nprivate void SetupFuncMonitor()\r\n{\r\n  InitFunc();\r\n\r\n  Func.ProjectInfo.CustomerId            = StepValStr(\"Monitor>CustomerId\");\r\n  Func.ProjectInfo.CustomerName          = StepValStr(\"Monitor>CustomerName\");\r\n  Func.ProjectInfo.ProjectId             = StepValStr(\"Monitor>ProjectId\");\r\n  Func.ProjectInfo.ProjectName           = StepValStr(\"Monitor>ProjectName\");\r\n  Func.ProjectInfo.IterationId           = StepValStr(\"Monitor>IterationId\");\r\n  Func.ProjectInfo.TestCaseId            = StepValStr(\"Monitor>TestCaseId\") + \"-\" + StringUtility.RandomString(3); \r\n  Func.ProjectInfo.ApplicationVersion    = StepValStr(\"Monitor>ApplicationVersion\");\r\n  Func.ProjectInfo.TechnologyApp         = \"Web\"; //Change this with the proper value\r\n  Func.ProjectInfo.GsqaService           = GsqaService.ATM; //TIA;ATM;Services\r\n  Func.ProjectInfo.HumanRelationRate     = StepValDouble(\"Monitor>HumanRelationRate\"); \r\n  Func.ProjectInfo.DeploymentEnvironment = DevEnvironment.QA; //DEV;QA;STG;PRD\r\n  Func.ProjectInfo.IsGreenHeartPrdMode   = StepValBool(\"Monitor>IsGreenHeartPrdMode\");\r\n  \r\n  ResetStartTime();\r\n}\r\n\r\nprivate void ValidationPrices()\r\n{\r\n  float product1           = float.Parse(StepVal(\"ShoppingCart>Product1\"), System.Globalization.CultureInfo.InvariantCulture.NumberFormat);\r\n  float product2           = float.Parse(StepVal(\"ShoppingCart>Product2\"), System.Globalization.CultureInfo.InvariantCulture.NumberFormat);\r\n  float product1_2         = float.Parse(StepVal(\"ShoppingCart>Product1_2\"), System.Globalization.CultureInfo.InvariantCulture.NumberFormat);\r\n  float total              = float.Parse(StepVal(\"ValidateTotals>Total\"), System.Globalization.CultureInfo.InvariantCulture.NumberFormat);\r\n  float roundSumValidation = (float)Math.Round(((product1 * 2) + (product2 * 3)),2);\r\n \r\n  LogTextToGH(\"Product value 1\", \"The price of product 1 shown in the search page was: $\" + product1.ToString());\r\n  LogTextToGH(\"Product value 2\", \"The price of product 2 shown in the search page was: $\" + product2.ToString()); \r\n  LogTextToGH(\"Total of both values\", \"The total of both products as shown in the search page was: $\" + product1_2.ToString());\r\n  LogTextToGH(\"Total after increasing number of products\", \"The total value shown after increasing the product quantities is now: $\" + total.ToString());\r\n  \r\n  if(product1_2<total)\r\n  {\r\n    LogTextToGH(\"Comparison of total fields\", \" Good!. After increasing the product quantities, we verified that the new total is greater than the initial total shown\");\r\n  }\r\n  else\r\n  {\r\n    throw new Exception (\"Error: The total value after increasing the quantities of the products is not greater than the initial total value\");\r\n  }\r\n        \r\n  if(roundSumValidation == total)\r\n  {\r\n    EvidencesManager.TakeEvidence(driver, \"Total comparison after increasing the products\", \"Good!. The total value after increasing the quantity of the first product and the second product to 2 and 3 respectively, matches the total value shown\", GetEvidencePath(), true);\r\n    //LogPictureToGH(\"Total comparison after increasing the products\", \"The total value after increasing the quantity of product 1 to 2 and product 2 to 3 coincides with the total value displayed\");\r\n  }\r\n  else\r\n  { \r\n    throw new Exception (\"Error: The total value after increasing the amount of product 1 to 2 and product 2 to 3 does not match the total value displayed\");\r\n  }\r\n}",
